{
    "version": "0.0.1",
    "description": "一个web项目",
    "author": "末烛",
    "license": "MIT",
    "bugs": {
        "url": "https://gitee.com/endcandle/vue-easy-admin/issues"
    },
    "repository": {
        "type": "git",
        "url": "https://gitee.com/endcandle/vue-easy-admin.git"
    },
    "scripts": {
        "serve": "vue-cli-service serve",
        "build": "vue-cli-service build",
        "mock": "nodemon --watch mocks/ -e js mocks/app.js",
        "test:unit": "vue-cli-service test:unit",
        "lint": "vue-cli-service lint",
        "lint:style": "stylelint src/**/*.{html,vue,css,scss} --fix",
        "analyzer": "cross-env use_analyzer=true vue-cli-service build",
        "commit": "git-cz",
        "test": "vue-cli-service build --mode test"
    },
    "dependencies": {
        "@antv/data-set": "^0.11.8",
        "@antv/g2": "^4.2.5",
        "ajv": "^8.11.0",
        "ajv-formats": "^2.1.1",
        "axios": "^0.27.2",
        "color": "^4.2.3",
        "core-js": "^3.24.1",
        "element-ui": "^2.15.9",
        "js-md5": "^0.7.3",
        "lodash-es": "^4.17.21",
        "nprogress": "^0.2.0",
        "reseter.css": "^2.0.0",
        "vue": "^2.6.14",
        "vue-i18n": "^8.27.2",
        "vue-router": "^3.5.4",
        "vuex": "^3.6.2",
        "wangeditor": "^4.7.15"
    },
    "devDependencies": {
        "@vue/cli-plugin-babel": "^5.0.8",
        "@vue/cli-plugin-eslint": "^5.0.8",
        "@vue/cli-plugin-router": "^5.0.8",
        "@vue/cli-plugin-unit-jest": "^5.0.8",
        "@vue/cli-plugin-vuex": "^5.0.8",
        "@vue/cli-service": "^5.0.8",
        "@vue/eslint-config-prettier": "^7.0.0",
        "@vue/test-utils": "^1.3.0",
        "babel-eslint": "^10.1.0",
        "chalk": "^4.1.2",
        "commitizen": "^4.2.5",
        "compression-webpack-plugin": "^10.0.0",
        "cross-env": "^7.0.3",
        "css-unicode-loader": "^1.0.3",
        "cz-conventional-changelog": "^3.3.0",
        "eslint": "^7.32.0",
        "eslint-config-alloy": "^3.10.0",
        "eslint-plugin-prettier": "^4.2.1",
        "eslint-plugin-vue": "^9.3.0",
        "express": "^4.18.1",
        "lint-staged": "^13.0.3",
        "mockjs": "^1.1.0",
        "nodemon": "^2.0.19",
        "prettier": "^2.7.1",
        "sass": "^1.54.3",
        "sass-loader": "^13.0.2",
        "stylelint": "^14.10.0",
        "stylelint-config-prettier-scss": "^0.0.1",
        "stylelint-config-property-sort-order-smacss": "^9.0.0",
        "stylelint-config-recommended-vue": "^1.4.0",
        "stylelint-config-standard-scss": "^5.0.0",
        "stylelint-order": "^5.0.0",
        "stylelint-webpack-plugin": "^3.3.0",
        "vue-eslint-parser": "^9.0.3",
        "vue-template-compiler": "^2.6.14",
        "webpack-bundle-analyzer": "^4.5.0"
    },
    "config": {
        "commitizen": {
            "path": "./node_modules/cz-conventional-changelog"
        }
    },
    "gitHooks": {
        "pre-commit": "lint-staged",
        "commit-msg": "node scripts/verify-commit-msg.js"
    },
    "lint-staged": {
        "*.{js,jsx,vue}": [
            "npm run lint:style",
            "vue-cli-service lint",
            "git add"
        ]
    }
}
